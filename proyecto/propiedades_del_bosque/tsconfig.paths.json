      "@assets/*": [ "src/assets/*" ],
      "@components/*": [ "src/components/*" ],
      "@pages/*": [ "src/pages/*" ],
      "@routes/*": [ "src/routes/*" ],
      "@services/*": [ "src/services/*" ]
